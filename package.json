{
  "name": "incubate",
  "version": "0.0.0",
  "description": "A habit-building game that rewards proven commitment, not intention.",
  "author": "Alireza Safaeirad <frontendmonster@gmail.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.27.0",
  "type": "module",
  "engines": {
    "node": "24.12.0"
  },
  "imports": {
    "#convex/api": "./convex/_generated/api.js",
    "#models/*": "./src/models/*.ts",
    "#storybook/preview": "./.storybook/preview.ts",
    "#lib/cn": "./src/lib/cn.ts",
    "#lib/form": "./src/lib/form/index.ts",
    "#components/*": "./src/components/*.tsx",
    "#ui/*": "./src/ui/*.tsx"
  },
  "scripts": {
    "lint": "eslint",
    "vite:dev": "vite",
    "convex:dev": "convex dev",
    "dev": "npm-run-all --parallel vite:dev convex:dev",
    "typecheck": "tsc -p tsconfig.app.json",
    "build": "pnpm run typecheck && vite build",
    "preview": "vite preview",
    "test": "vitest --project=unit",
    "spellcheck": "cspell .",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:test": "vitest --project=storybook"
  },
  "devDependencies": {
    "@convex-dev/eslint-plugin": "1.1.1",
    "@fullstacksjs/eslint-config": "13.13.3",
    "@storybook/addon-a11y": "10.2.3",
    "@storybook/addon-docs": "10.2.3",
    "@storybook/addon-vitest": "10.2.3",
    "@storybook/react-vite": "10.2.3",
    "@tailwindcss/vite": "4.1.18",
    "@tanstack/react-devtools": "0.9.0",
    "@tanstack/react-router-devtools": "1.144.0",
    "@tanstack/router-plugin": "1.145.2",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/browser-playwright": "4.0.16",
    "@vitest/coverage-v8": "4.0.16",
    "cspell": "9.6.2",
    "eslint": "9.39.2",
    "npm-run-all2": "8.0.4",
    "playwright": "1.57.0",
    "prettier": "3.7.4",
    "storybook": "10.2.3",
    "tailwindcss": "4.1.18",
    "vite": "7.3.0",
    "vitest": "4.0.16"
  },
  "dependencies": {
    "@base-ui/react": "1.0.0",
    "@convex-dev/auth": "0.0.90",
    "@fontsource-variable/inter": "5.2.8",
    "@fullstacksjs/toolbox": "4.22.0",
    "@tabler/icons-react": "3.36.1",
    "@tanstack/react-form": "1.28.0",
    "@tanstack/react-router": "1.144.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "convex": "1.31.7",
    "date-fns": "4.1.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "3.4.0",
    "valibot": "1.2.0"
  }
}
